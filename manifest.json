{
  "name": "Visualiz",
  "version": "1.0",
  "description": "Build an Extension!",
  "permissions": ["storage", "declarativeContent", "activeTab", "<all_urls>", "tabs"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [ 
    { 
      "js": ["jquery.js"],
      "matches": [ "http://*/*", "https://*/*"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}